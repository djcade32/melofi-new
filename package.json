{
  "name": "melofi-new",
  "version": "0.1.0",
  "main": "electron.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test:unit": "jest",
    "emulators:start": "firebase emulators:start --only auth,firestore",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless",
    "test:e2e": "concurrently \"firebase emulators:start\" \"NODE_ENV=test start-server-and-test dev http://localhost:3000 cy:open\"",
    "test:e2e:headless": "concurrently \"firebase emulators:start\" \"NODE_ENV=test start-server-and-test dev http://localhost:3000 cy:run\"",
    "electron": "electron .",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run electron\"",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.2",
    "@react-oauth/google": "^0.12.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "cypress-real-events": "^1.13.0",
    "events": "^3.3.0",
    "firebase": "^10.14.0",
    "firebase-admin": "^12.7.0",
    "idb": "^8.0.0",
    "next": "14.2.13",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-draggable": "^4.4.6",
    "react-full-screen": "^1.1.1",
    "react-icons": "^5.3.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@types/cypress": "^0.1.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.1.0",
    "cypress": "^13.15.0",
    "electron": "^32.1.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "start-server-and-test": "^2.0.8",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "wait-on": "^8.0.1"
  },
  "type": "module"
}
